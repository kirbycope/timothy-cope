<div class="container-fluid">
        <div class="row">
            <!-- Side-Nav -->
            <%- include ../../partials/aside/admin.ejs %>
            <!-- Top Menu -->
            <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
                <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
                    <h1 class="h2"><%= title %></h1>
                </div>
                <!-- Main Section-->
                <section>
                    <h2>All Blog Posts</h2>
                    <div id="posts" class="table-responsive">
                        <input id="inputFilter" type="text" class="search form-control" placeholder="Filter">
                        <table class="table table-striped table-sm text-left">
                            <thead>
                                <tr>
                                    <th class="sort" data-sort="slug">Slug</th>
                                    <th class="sort" data-sort="category">Category</th>
                                    <th class="sort" data-sort="content">Content</th>
                                    <th class="sort" data-sort="date">Date</th>
                                    <th class="sort" data-sort="description">Description</th>
                                    <th class="sort" data-sort="thumbnail">Thumbnail</th>
                                    <th class="sort" data-sort="title">Title</th>
                                    <th>Actions</th>
                                </tr>
                            </thead>
                            <tbody id="tbodyAllBlogPosts" class="list">
                            </tbody>
                        </table>
                    </div>
                </section>
            </main>
        </div>
    </div>